{
    broker: {
        listen_addr: ":1883"
        tls_listen_addr: ":8883"
        tls_server_cert: ""
        tls_server_key: ""

        stats_interval: "10s"
        subscriptions_workers: 10
        echo: true

        allow_anonymous: true
        max_queued_messages: 100
    }

    peers: {
        listen_addr: ":9090"
        tcp_nodelay: true
        keepalive: true
        io_timeout: "2s"
        echo: true
    }

    redis: {
        servers: [
        ]
    }
}
